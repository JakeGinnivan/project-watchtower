    "build:debug": "tsc -p tsconfig-debug.json",
    "build:complete": "npm run clean && npm run lint && npm run test -- --coverage --verbose && npm run build",
    "pretest:base": "npm run clean:lighthouse",
    "test:base": "jest --runInBand --forceExit --config jest.json",
    "test": "npm run test:base -- --silent",
    "test:debug": "node --debug ./node_modules/jest/bin/jest.js --runInBand --config jest-debug.json",
    "test:verbose": "npm run test:base -- --verbose",
    "coverage": "npm run test -- --coverage",
    "watch": "npm run build -- --watch",
    "clean:lighthouse": "rimraf node_modules/lighthouse/**/*.ts",
    "pwt": "node bin/index.js"
  },
