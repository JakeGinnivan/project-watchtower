    "scripts": {
        "clean": "npm run clean:lighthouse && rimraf dist-es \"{lib,test}/**/*.{js,map,d.ts}\"",
        "lint": "tslint 'lib/**/*.ts' --type-check --project ./tsconfig.json -e '**/*.d.ts'",
        "lint:prettier": "prettier --single-quote=true --tab-width=4 --trailing-comma=all --use-tabs=false --print-width=100 --no-semi --write \"./lib/**/*.ts\" \"./lib/**/*.tsx\" \"./server/**/*.ts\" \"./server/**/*.tsx\"",
        "build": "tsc && tsc -p ./tsconfig-es2015.json",
        "build:complete": "npm run clean && npm run lint && npm run build && npm run test -- --coverage --verbose",
        "pretest:base": "npm run clean:lighthouse",
        "test:base": "jest --config jest.json",
        "test": "npm run test:base -- --silent",
        "pretest:debug": "npm run build",
        "test:debug": "yarn pwt test debug",
        "test:verbose": "npm run test:base -- --verbose",
        "coverage": "npm run test -- --coverage",
        "watch": "npm run build -- --watch",
        "clean:lighthouse": "rimraf node_modules/lighthouse/**/*.ts",
        "pwt": "node bin/index.js",
        "--demo commands--": "",
        "prebuild:demo": "npm run clean:lighthouse && yarn build",
        "build:demo": "yarn pwt build -p demo",
        "prewatch:demo": "npm run clean:lighthouse && yarn build",
        "watch:demo": "yarn pwt watch -p demo",
        "prewatch:ssrdemo": "npm run clean:lighthouse && yarn build",
        "watch:ssrdemo": "ENABLE_SSR=true yarn pwt watch -p demo",
        "debug:ssrdemo": "ENABLE_SSR=true yarn pwt start debug -p demo",
        "predebug-brk:ssrdemo": "yarn build:demo",
        "debug-brk:ssrdemo": "ENABLE_SSR=true yarn pwt start debug-brk -p demo",
        "prewatch-server:ssrdemo": "yarn build:demo",
        "watch-server:ssrdemo": "ENABLE_SSR=true yarn pwt watch server -p demo",
        "test:demo": "yarn pwt test -p demo",
        "prestats:demo": "yarn build:demo",
        "stats:demo": "yarn pwt stats verbose -p demo",
        "expore-bundle:demo": "yarn pwt explore-bundle -p demo"
    },
    "main": "lib/index.js",
    "bin": {
        "project-watchtower": "bin/index.js",
        "pwt": "bin/index.js"
    },
