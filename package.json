{
    "name": "project-watchtower",
    "version": "0.12.22",
    "license": "MIT",
    "description": "",
    "publishConfig": {
        "registry": "https://nexus.swmdigital.io/repository/js-private"
    },
    "scripts": {
        "install-example-dependencies": "find ./examples/ -maxdepth 1 -type d \\( ! -name examples \\) -exec bash -c \"cd '{}' && yarn\" \\;",
        "clean": "npm run clean:lighthouse && rimraf dist-es examples/*/client/*.d.ts examples/*/client/*.js examples/*/server/*.d.ts examples/*/server/*.js examples/*/common/*.d.ts examples/*/common/*.js",
        "lint": "tslint 'lib/**/*.ts' --type-check --project ./tsconfig.json -e '**/*.d.ts'",
        "lint:prettier": "prettier --single-quote=true --tab-width=4 --trailing-comma=all --use-tabs=false --print-width=100 --no-semi --write \"./lib/**/*.ts\" \"./lib/**/*.tsx\" \"./server/**/*.ts\" \"./server/**/*.tsx\"",
        "build": "tsc -p ./tsconfig-cjs.json && tsc -p ./tsconfig-es2015.json",
        "compile": "tsc",
        "build:complete": "npm run install-example-dependencies && npm run clean && npm run lint && npm run build && npm run test -- --coverage --verbose",
        "pretest:base": "npm run clean:lighthouse && npm run build",
        "test:base": "jest --config jest.json",
        "test": "npm run test:base -- --silent",
        "test:debug": "yarn pwt test debug",
        "test:verbose": "npm run test:base -- --verbose",
        "coverage": "npm run test -- --coverage",
        "watch": "npm run build -- --watch",
        "clean:lighthouse": "rimraf node_modules/lighthouse/**/*.ts",
        "pwt": "node bin/index.js",
        "~~ example projects ~~": "",
        "~~~ client-only ~~~": "",
        "prebuild:client-only-example": "npm run clean:lighthouse && yarn build",
        "prewatch:client-only-example": "npm run clean:lighthouse && yarn build",
        "prestats:client-only-example": "yarn build:client-only-example",
        "build:client-only-example": "yarn pwt build -p examples/client-only",
        "watch:client-only-example": "yarn pwt watch -p examples/client-only",
        "stats:client-only-example": "yarn pwt stats verbose -p examples/client-only",
        "explore-bundle:client-only-example": "yarn pwt explore-bundle -p examples/client-only",
        "~~~ with-server ~~~": "",
        "prebuild:with-server-example": "npm run clean:lighthouse && yarn build",
        "prewatch:with-server-example": "npm run clean:lighthouse && yarn build",
        "prestats:with-server-example": "yarn build:with-server-example",
        "build:with-server-example": "yarn pwt build -p examples/with-server",
        "watch:with-server-example": "yarn pwt watch server -p examples/with-server",
        "stats:with-server-example": "yarn pwt stats verbose -p examples/with-server",
        "explore-bundle:with-server-example": "yarn pwt explore-bundle -p examples/with-server",
        "~~~ simple-ssr ~~~": "",
        "prebuild:simple-ssr-example": "npm run clean:lighthouse && yarn build",
        "prewatch:simple-ssr-example": "npm run clean:lighthouse && yarn build",
        "prestats:simple-ssr-example": "yarn build:simple-ssr-example",
        "build:simple-ssr-example": "yarn pwt build -p examples/simple-ssr",
        "watch:simple-ssr-example": "yarn pwt watch server -p examples/simple-ssr",
        "stats:simple-ssr-example": "yarn pwt stats verbose -p examples/simple-ssr",
        "explore-bundle:simple-ssr-example": "yarn pwt explore-bundle -p examples/simple-ssr",
        "~~~ ssr-with-routing ~~~": "",
        "prebuild:ssr-with-routing-example": "npm run clean:lighthouse && yarn build",
        "prewatch:ssr-with-routing-example": "npm run clean:lighthouse && yarn build",
        "prestats:ssr-with-routing-example": "yarn build:ssr-with-routing-example",
        "build:ssr-with-routing-example": "yarn pwt build -p examples/ssr-with-routing",
        "watch:ssr-with-routing-example": "yarn pwt watch server -p examples/ssr-with-routing",
        "stats:ssr-with-routing-example": "yarn pwt stats verbose -p examples/ssr-with-routing",
        "explore-bundle:ssr-with-routing-example": "yarn pwt explore-bundle -p examples/ssr-with-routing",
        "~~~ ssr-with-data-loading-using-redux ~~~": "",
        "prebuild:ssr-with-data-loading-using-redux-example": "npm run clean:lighthouse && yarn build",
        "prewatch:ssr-with-data-loading-using-redux-example": "npm run clean:lighthouse && yarn build",
        "prestats:ssr-with-data-loading-using-redux-example": "yarn build:ssr-with-data-loading-using-redux-example",
        "build:ssr-with-data-loading-using-redux-example": "yarn pwt build -p examples/ssr-with-data-loading-using-redux",
        "watch:ssr-with-data-loading-using-redux-example": "yarn pwt watch server -p examples/ssr-with-data-loading-using-redux",
        "stats:ssr-with-data-loading-using-redux-example": "yarn pwt stats verbose -p examples/ssr-with-data-loading-using-redux",
        "explore-bundle:ssr-with-data-loading-using-redux-example": "yarn pwt explore-bundle -p examples/ssr-with-data-loading-using-redux",
        "~~~ emotion-css-in-js ~~~": "",
        "prebuild:emotion-css-in-js-example": "npm run clean:lighthouse && yarn build",
        "prewatch:emotion-css-in-js-example": "npm run clean:lighthouse && yarn build",
        "prestats:emotion-css-in-js-example": "yarn build:emotion-css-in-js-example",
        "build:emotion-css-in-js-example": "yarn pwt build -p examples/emotion-css-in-js",
        "watch:emotion-css-in-js-example": "yarn pwt watch server -p examples/emotion-css-in-js",
        "stats:emotion-css-in-js-example": "yarn pwt stats verbose -p examples/emotion-css-in-js",
        "explore-bundle:emotion-css-in-js-example": "yarn pwt explore-bundle -p examples/emotion-css-in-js"
    },
    "main": "lib/index.js",
    "bin": {
        "project-watchtower": "bin/index.js",
        "pwt": "bin/index.js"
